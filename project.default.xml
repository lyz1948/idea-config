<application>
  <component name="ProjectManager">
    <defaultProject>
      <component name="MavenImportPreferences">
        <option name="generalSettings">
          <MavenGeneralSettings>
            <option name="localRepository" value="$USER_HOME$/maven/repo" />
            <option name="mavenHome" value="$APPLICATION_HOME_DIR$/plugins/maven/lib/maven3" />
            <option name="userSettingsFile" value="$USER_HOME$/apache-maven-3.8.1/conf/settings.xml" />
          </MavenGeneralSettings>
        </option>
      </component>
      <component name="PropertiesComponent">
        <property name="settings.editor.selected.configurable" value="MavenSettings" />
      </component>
    </defaultProject>
  </component>
</application>